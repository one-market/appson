{
  "name": "@onemarket/appson",
  "description": "Frontend framework to manage React applications",
  "version": "0.4.6",
  "main": "./lib/index.js",
  "browser": "./lib/index.js",
  "module": "./es/index.js",
  "jsnext:main": "./es/index.js",
  "typings": "./index.d.ts",
  "files": [
    "lib",
    "es",
    "index.d.ts"
